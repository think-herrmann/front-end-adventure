{
  "presets": [
    [
      "@babel/preset-env",
      {
        "modules": false,
        shippedProposals: true,
        "targets": {
          "browsers": "> 1%",
          "uglify": true
        },
        "useBuiltIns": "entry"
      }
    ],
    "@babel/preset-react"
  ],
  "env": {
    "test": {
      "presets": ["@babel/preset-env"]
    }
  },
  "plugins": [
    "syntax-dynamic-import",
    "transform-regenerator",
    "react-hot-loader/babel",
    [
      "transform-class-properties",
      {
        "spec": true
      }
    ]
  ]
}
